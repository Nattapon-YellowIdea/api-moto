{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "max-len": ["error", { "code": 1000 }],
        "no-underscore-dangle": "off",
        "linebreak-style": 0
    }
    
}
